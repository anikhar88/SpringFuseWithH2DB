+        this.title = title;
+    }
+
+    public Book title(String title) {
+        setTitle(title);
+        return this;
+    }
+
+    // -- [numberOfPages] ------------------------
+
+    @Digits(integer = 10, fraction = 0)
+    @NotNull
+    @Column(name = "NUMBER_OF_PAGES", nullable = false, precision = 10)
+    public Integer getNumberOfPages() {
+        return numberOfPages;
+    }
