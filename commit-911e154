+			<ui:insert/>
+		</p:calendar>
+		<p:tooltip for="#{id}" value="#{tooltip}" showEffect="slide" rendered="#{not empty tooltip}"/>
+	</h:panelGroup>
+	<h:outputText value="#{value}" rendered="#{readonly and (empty rendered or rendered)}">
+		<f:convertDateTime pattern="yyyy-MM-dd HH:mm" />
+	</h:outputText>
+</ui:composition>
