+body {
+	font-family: Verdana, Arial, Helvetica, sans-serif;
+	font-size: 12px;
+	min-width:1200px;	
+	background-color : #d0d3dc;
+}
+
+/* LABEL */
+form label.required {
+	font-weight: bold;
+}
+
+form .required-pseudo-label {
+	font-weight: bold;
+}
+
+div .ui-menu.ui-menubar {
+	background-color:transparent;
