+        StringBuilder ret = new StringBuilder();
+        appendIfNotEmpty(ret, auditLog.getAuthor());
+        appendIfNotEmpty(ret, auditLog.getEvent());
+        appendIfNotEmpty(ret, auditLog.getStringAttribute1());
+        return ret.toString();
+    }
