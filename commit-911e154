+			</h:panelGrid>
+		</p:fieldset>
+		<p:fieldset id="filtersFieldset" legend="#{msg.search_criteria}" toggleable="true">
+			<input type="hidden" name="_cid" value="#{conversationManager.currentConversation.cid}"/>
