+@Constraint(validatedBy = FixedLengthValidator.class)
+@Documented
+public @interface FixedLength {
+    int length() default 0;
+
+    boolean nullable() default true;
+
+    String message() default "{com.h2.demo.validation.FixedLength.message}";
+
